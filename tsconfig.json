{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "typings.d.ts"],
    "exclude": ["node_modules"]
}
